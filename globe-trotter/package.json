{
  "name": "globe-trotter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:client": "vite",
    "dev:server": "nodemon --watch 'src/server' --ext 'ts,json' --exec 'npm run build:server && node dist-server/server.js'",
    "db:init": "npm run build:server && node dist-server/database.js",
    "build": "tsc -b && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react-big-calendar": "^1.16.2",
    "@types/recharts": "^1.8.29",
    "@types/sqlite3": "^3.1.11",
    "amadeus": "^11.0.0",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "moment": "^2.30.1",
    "react": "^19.1.1",
    "react-big-calendar": "^1.19.4",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  }
}
